{
  "name": "crc64-ecma",
  "version": "1.0.0",
  "description": "Calculate CRC64/XZ hashes for NodeJS.",
  "license": "GPL-2.0",
  "author": "AkiVer",
  "homepage": "https://github.com/akiver/crc64-ecma",
  "repository": {
    "type": "git",
    "url": "https://github.com/akiver/crc64-ecma.git"
  },
  "main": "./dist/index.js",
